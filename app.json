{
  "expo": {
    "name": "ecosenseApp",
    "slug": "expo-template-default",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/logoClaro.png",
    "scheme": "ecosense",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./src/assets/logoClaro.png",
      "resizeMode": "contain",
      "backgroundColor": "#2196F3"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/logoClaro.png",
        "backgroundColor": "#2196F3"
      },
      "package": "com.vicciz.ecosense"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./src/assets/logoClaro.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "f58fd812-12ab-4052-bf22-6f3c38fd1f3d"
      }
    },
    "owner": "vicci22",
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/f58fd812-12ab-4052-bf22-6f3c38fd1f3d"
    }
  }
}
